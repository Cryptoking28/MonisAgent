{
  "name": "@monisagent/koa",
  "version": "5.0.0",
  "description": "Koa instrumentation for the Monis Agent Node agent",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "test": "npm run unit && npm run versioned",
    "unit": "tap tests/unit/**/*.tap.js --no-coverage --reporter classic",
    "versioned": "npm run versioned:npm7",
    "versioned:npm6": "versioned-tests --minor -i 2 'tests/versioned/*.tap.js'",
    "versioned:npm7": "versioned-tests --minor --all -i 2 'tests/versioned/*.tap.js'",
    "lint": "eslint *.js lib tests"
  },
  "author": "Monis Agent Node.js agent team <nodejs@monisagent.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@koa/router": "^8.0.0",
    "@monisagent/test-utilities": "^5.1.0",
    "eslint": "^5.8.0",
    "koa": "^2.6.1",
    "koa-route": "^3.2.0",
    "koa-router": "^7.4.0",
    "monisagent": "^6.11.0",
    "semver": "^5.7.1",
    "tap": "^15.0.9"
  },
  "dependencies": {
    "methods": "^1.1.2"
  },
  "peerDependencies": {
    "monisagent": ">=6.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cryptoking28/monisagent-koa.git"
  }
}
