{
  "name": "@monisagent/superagent",
  "version": "5.0.1",
  "description": "Monis Agent instrumentation for the `superagent` package.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js lib tests",
    "lint:fix": "eslint --fix *.js lib tests",
    "unit": "tap tests/unit/**/*.tap.js --no-coverage",
    "versioned": "npm run versioned:npm7",
    "versioned:npm6": "versioned-tests --minor -i 2 'tests/versioned/*.tap.js'",
    "versioned:npm7": "versioned-tests --minor --all -i 2 'tests/versioned/*.tap.js'",
    "test": "npm run lint && npm run unit && npm run versioned"
  },
  "author": "Monis Agent Node.js Agent Team <nodejs@monisagent.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@monisagent/eslint-config": "^0.0.1",
    "@monisagent/test-utilities": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "monisagent": "^8.1.0",
    "prettier": "^2.3.2",
    "semver": "^5.5.0",
    "superagent": "^3.8.3",
    "tap": "^15.0.9"
  },
  "engines": {
    "node": ">=12.0"
  },
  "peerDependencies": {
    "monisagent": ">=6.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cryptoking28/monisagent-superagent.git"
  },
  "files": [
    "*.md",
    "index.js",
    "nr-hooks.js",
    "lib/**",
    "tests/versioned/*.js"
  ]
}
